{
  "usesTypeScript": true,
  "usesStyledComponents": true,
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "withIndex": true,
      "customTemplates": {
        "index": "templates/components/index.tsx",
        "component": "templates/components/TemplateName.tsx",
        "story": "templates/components/TemplateName.story.tsx",
        "style": "templates/components/TemplateName.styled.tsx",
        "test": "templates/components/TemplateName.test.tsx"
      }
    },
    "view": {
      "path": "src/views",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true,
      "withIndex": true,
      "customTemplates": {
        "index": "templates/views/index.tsx",
        "component": "templates/views/TemplateName.tsx",
        "lazy": "templates/views/TemplateName.lazy.tsx",
        "story": "templates/views/TemplateName.story.tsx",
        "style": "templates/views/TemplateName.style.css",
        "test": "templates/views/TemplateName.test.tsx"
      }
    },
    "container": {
      "path": "src/containers",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": true,
      "withIndex": true,
      "customTemplates": {
        "index": "templates/containers/index.js",
        "component": "templates/containers/TemplateName.jsx",
        "lazy": "templates/containers/TemplateName.lazy.jsx",
        "story": "templates/containers/TemplateName.story.jsx",
        "style": "templates/containers/TemplateName.style.css",
        "test": "templates/containers/TemplateName.test.jsx"
      }
    }
  }
}